INSERT INTO public.lo3_rni_deelnemer
	(deelnemer_code, deelnemer_oms, tabel_regel_start_datum)
	VALUES (101, 'Belastingdienst (inzake heffingen en toeslagen)', 20220101)
	ON CONFLICT(deelnemer_code) DO
		UPDATE SET  deelnemer_oms = EXCLUDED.deelnemer_oms,
					tabel_regel_start_datum=EXCLUDED.tabel_regel_start_datum;

